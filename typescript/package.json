    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "schedule": "ts-node src/index.ts --schedule",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\""
